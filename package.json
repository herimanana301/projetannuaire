{
  "name": "template-fullstack",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "setup": "npm i && husky install && npm --prefix ./frontend i",
    "migrate": "cd backend/ && node migrate.js && cd ..",
    "dev": "concurrently -n front,back -c green,yellow -t \"HH:mm:ss\" -p \"{name} {time}\" \"npm --prefix ./frontend run dev\" \"npm --prefix ./backend run dev\"",
    "dev-front": "npm --prefix ./frontend run dev",
    "dev-back": "npm --prefix ./backend run dev",
    "lint": "npm --prefix ./frontend run lint",
    "fix": "npm --prefix ./frontend run fix && npm --prefix ./backend run fix"
  },
  "keywords": [],
  "author": "Wild Code School",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^7.0.0",
    "husky": "^7.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "history": "^5.3.0",
    "jsonwebtoken": "^9.0.0",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "react-router-dom": "^6.4.5",
    "sort-by": "^0.0.2"
  }
}
